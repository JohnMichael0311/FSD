<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Partner Restaurant Dashboard</title>
</head>
<body>
    <h1>Partner Restaurant Dashboard</h1>
    <p>Welcome, here you can view user information:</p>
    <table border="1">
        <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
                <th>Role</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(function(user) { %>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.email %></td>
                <td><%= user.role %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
